Maybe :: enum(*V)
    Just *V
    None
end

orDefault :: pu maybe, a ->
    case maybe do
        Just x -> x end
        None -> a end
    end
end

flatten :: pu maybe ->
    case maybe do
        Just x -> x end
        None -> Maybe.None end
    end
end

isJust :: pu maybe -> bool
    case maybe do
        Just _ -> true end
        None -> false end
    end
end

isNone :: pu maybe -> bool
    not isJust' maybe
end
